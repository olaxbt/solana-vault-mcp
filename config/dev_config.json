{
  "version": "1.0.0",
  "app_name": "Solana Vault MCP",
  "environment": "development",
  "rate_limits": {
    "default": 60,
    "wallet_transfer": 10
  },
  "cache_settings": {
    "transaction_cache_size": 100,
    "balance_cache_ttl_seconds": 30
  },
  "allowed_actions": [
    "wallet.info",
    "wallet.balance",
    "wallet.transfer",
    "wallet.transactions",
    "wallet.token_balance"
  ]
} 